{
  "name": "mammut-alex/lardi-trans",
  "description": "Php API client for Lardi-Trans",
  "keywords": [
    "api",
    "lardi-trans"
  ],
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Alex Kovalchuk",
      "email": "alexk@cargofy.com"
    }
  ],
  "require": {
    "php": ">=7.1",
    "guzzlehttp/guzzle": ">=6.0",
    "ext-json": "*",
    "ext-simplexml": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^7"
  },
  "autoload": {
    "psr-4": {
      "MammutAlex\\LardiTrans\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
